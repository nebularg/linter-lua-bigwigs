{
  "name": "linter-lua-bigwigs",
  "version": "1.0.0",
  "description": "Lint BigWigs modules.",
  "repository": "https://github.com/nebularg/linter-lua-bigwigs",
  "readmeFilename": "README.md",
  "configSchema": {
    "executablePath": {
      "type": "string",
      "default": "lua",
      "description": "The executable path to Lua."
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "main": "./lib/init",
  "engines": {
    "atom": ">=1.14.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.5.0"
  },
  "package-deps": [
    "linter:2.0.0"
  ]
}
